{
	"name": "power-bi-thems-extension",
	"displayName": "Power Bi Themes Extension",
	"description": "This is extension for managing Power BI styles.",
	"version": "0.0.5",
	"publisher": "AlexGlushko",
	"repository": {
		"type": "git",
		"url": "https://github.com/Badcat330/PowerBiThems.git"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:power-bi-thems-extension.DbConnection"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "power-bi-thems-extension.AlternativeVisualizers",
				"title": "BIS: Alternative Visualizers"
			},
			{
				"command": "power-bi-thems-extension.DbConnection",
				"title": "BIS: Connect to data base"
			},
			{
				"command": "power-bi-thems-extension.CreateTag",
				"title": "BIS: Create tag"
			},
			{
				"command": "power-bi-thems-extension.changeTag",
				"title": "BIS: Change tag data"
			},
			{
				"command": "power-bi-thems-extension.DeleteTag",
				"title": "BIS: Delete tag from data base"
			},
			{
				"command": "power-bi-thems-extension.Visualize",
				"title": "BIS: Visualize test report"
			},
			{
				"command": "power-bi-thems-extension.SaveFile",
				"title": "BIS: Save file to data base"
			},
			{
				"command": "power-bi-thems-extension.changeMetadata",
				"title": "BIS: Metadata change"
			},
			{
				"command": "power-bi-thems-extension.AddTag",
				"title": "BIS: Add tag to file"
			},
			{
				"command": "power-bi-thems-extension.RemoveTag",
				"title": "BIS: Remove tag from file"
			},
			{
				"command": "power-bi-thems-extension.DeleteFile",
				"title": "BIS: Delete file from data base"
			},
			{
				"command": "power-bi-thems-extension.DownloadFile",
				"title": "BIS: Download file from data base"
			},
			{
				"command": "power-bi-thems-extension.ShowFileInformation",
				"title": "BIS: Show file information"
			},
			{
				"command": "power-bi-thems-extension.BackFileVersion",
				"title": "BIS: Back to file version"
			}
		],
		"configuration": {
			"title": "Power BI Themes",
			"properties": {
				"power-bi-thems-extension.Host": {
					"type": "string",
					"default": "thems.database.windows.net",
					"description": "Host of data base connection"
				},
				"power-bi-thems-extension.UserName": {
					"type": "string",
					"default": "",
					"description": "User name for data base connection"
				},
				"power-bi-thems-extension.Name": {
					"type": "string",
					"default": "thems_storage",
					"description": "Name of data base you would like to connect"
				},
				"power-bi-thems-extension.NormativeTestPath": {
					"type": "string",
					"default": "",
					"description": "Path to test normative report"
				},
				"power-bi-thems-extension.CustomTestPath": {
					"type": "string",
					"default": "",
					"description": "Path to test custom report"
				}
			}
		},
		"keybindings": [
			{
				"command": "power-bi-thems-extension.SaveFile",
				"key": "ctrl+alt+t",
				"mac": "shift+option+T",
				"when": "editorTextFocus"
			},
			{
				"command": "power-bi-thems-extension.Visualize",
				"key": "ctrl+alt+V",
				"mac": "shift+option+T"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"tedious": "^8.3.0"
	}
}