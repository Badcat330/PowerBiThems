{
	"name": "power-bi-thems-extension",
	"displayName": "Power Bi Thems Extension",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:power-bi-thems-extension.DbConnection"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "power-bi-thems-extension.AlternativeVisualizers",
				"title": "Alternative Visualizers"
			},
			{
				"command": "power-bi-thems-extension.DbConnection",
				"title": "Connect DB"
			},
			{
				"command": "power-bi-thems-extension.AddTag",
				"title": "Add tag"
			},
			{
				"command": "power-bi-thems-extension.RenameTag",
				"title": "Rename tag"
			},
			{
				"command": "power-bi-thems-extension.DeleteTag",
				"title": "Delete tag"
			}
		],
		"configuration": {
			"title": "Power BI Themes",
			"properties": {
				"power-bi-thems-extension.Host": {
					"type": "string",
					"default": "thems.database.windows.net",
					"description": "Host of data base connection"
				},
				"power-bi-thems-extension.Port": {
					"type": "integer",
					"default": 1433,
					"description": "Port for data base connection"
				},
				"power-bi-thems-extension.Name": {
					"type": "string",
					"default": "thems_storage",
					"description": "Name of data base you would like to connect"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"tedious": "^8.3.0"
	}
}
